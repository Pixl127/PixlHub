   Name = "Auto Break Party Crate",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)

   end,
})

--------------------------------------------------------
-- Egg Tab
--------------------------------------------------------
local AutoOpenEggToggle = EggTab:CreateToggle({
   Name = "Auto Open Egg",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)

   end,
})

--------------------------------------------------------
-- Misc Tab
--------------------------------------------------------

local function collectGifts()
   while task.wait(60) do
      for number = 1, 12, 1 do
         workspace:WaitForChild("__THINGS"):WaitForChild("__REMOTES"):WaitForChild("redeem free gift"):InvokeServer(number)
      end
   end
end

local toggle = false
local AutoCollectFreeGiftsToggle = MiscTab:CreateToggle({
   Name = "Auto Collect Free Gifts",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
      toggle = !toggle
      collectGifts()
   end,
})


--------------------------------------------------------
-- Script Tab
--------------------------------------------------------
local DestroyUiButton = MiscTab:CreateButton({
   Name = "Destroy UI",
   Callback = function()
      Rayfield:Destroy()
   end,
})

--------------------------------------------------------
-- Dev Tab
--------------------------------------------------------
local LoadInfiniteYieldButton = DevTab:CreateButton({
   Name = "Load Infinite Yield",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end,
})

