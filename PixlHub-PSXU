local args = {
	"b",
	"redeem free gift"
}
workspace:WaitForChild("__THINGS"):WaitForChild("__REMOTES"):WaitForChild("MAIN"):FireServer(unpack(args))

local args = {
	"a",
	"claim orbs"
}
workspace:WaitForChild("__THINGS"):WaitForChild("__REMOTES"):WaitForChild("MAIN"):FireServer(unpack(args))
